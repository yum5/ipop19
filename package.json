{
  "name": "iPOPDemo",
  "version": "1.0.0",
  "description": "demo",
  "main": "./build/main.js",
  "homepage": ".",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-regenerator"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@babel/types": "^7.0.0",
    "@material-ui/core": "^3.0.1",
    "@material-ui/icons": "^3.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-polyfill": "^6.26.0",
    "electron": "^11.5.0",
    "electron-redux": "^1.3.1",
    "electron-settings": "^3.2.0",
    "electron-store": "^2.0.0",
    "nanotimer": "^0.3.15",
    "node-cmd": "^3.0.0",
    "p-iteration": "^1.1.7",
    "react": "^16.4.2",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sftp-promises": "^1.6.0",
    "snapsvg-cjs": "^0.0.6"
  },
  "scripts": {
    "build": "concurrently \"cross-env NODE_ENV=development webpack --progress --color --config webpack/config.renderer.js\" \"cross-env NODE_ENV=development webpack --progress --color --config webpack/config.main.js\"",
    "build:production": "concurrently \"cross-env NODE_ENV=production webpack --progress --color --config webpack/config.renderer.js\" \"cross-env NODE_ENV=production webpack --progress --color --config webpack/config.main.js\"",
    "pack:linux": "npm run build:production && electron-packager build/ Demo --platform=linux --arch=x64 --version=1.8.6 --asar --package-manager yarn --overwrite --out dist/",
    "pack:mac": "npm run build:production && electron-packager build/ Demo --platform=darwin --arch=x64 --version=1.8.6 --asar --package-manager yarn --overwrite --out dist/",
    "start": "npm run build && electron build/main.js",
    "test": "jest --detectOpenHandles --forceExit",
    "lint": "eslint src test --ignore-pattern node_modules/ --ext .js,.jsx"
  },
  "devDependencies": {
    "chart.js": "^2.7.2",
    "chartjs-plugin-streaming": "^1.4.1",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "electron-builder": "^20.10.0",
    "electron-devtools-installer": "^2.2.3",
    "electron-packager": "^12.0.2",
    "electron-webpack": "^2.1.0",
    "electron-webpack-vue": "^2.1.2",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-eslint-comments": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.26.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "node-sass": "^4.9.0",
    "node-shell-parser": "^0.2.1",
    "react-test-renderer": "^16.5.2",
    "rxjs": "^6.0.0",
    "sass-loader": "^7.0.1",
    "sinon": "^6.2.0",
    "style-loader": "^0.21.0",
    "vue": "^1.0.28-csp",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "webpack-node-externals": "^1.7.2"
  }
}
